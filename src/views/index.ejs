<!DOCTYPE html>
<html lang="pt-BR">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Dynamic Tables</title>
  </head>

  <body>
    <table>
      <thead>
        <% headers.forEach(header=> {%>
        <th><%=header%></th>
        <%})%>
      </thead>

      <tbody>
        <% rows.forEach(row=> { %>
        <tr>
          <% row.forEach(item=> { %>
          <td><%=item%></td>
          <% }) %>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
